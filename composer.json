{
    "description": "Appliction by Zeko",
    "keywords": ["framework", "zeko"],
    "license": "ZEKOFRAMWORK",
    "type": "project",
    "require": {
        "symfony/http-foundation": "^7.0",
        "j4mie/idiorm": "^1.5"
    },
    "autoload": {
        "psr-4": {
            "Zeko\\": "app/",
             "widgetEngine\\": "widgetEngine/widgets/",
            "Zeko\\Core\\": "Zeko/"
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}